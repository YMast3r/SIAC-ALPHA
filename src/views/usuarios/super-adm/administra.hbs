<head>
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body class="bg-indigo-200">
    <header class="bg-white px-7 shadow">
        <div class="flex h-24 items-center justify-between">
            <button 
                class="-ml-1 rounded p-1 text-slate-950 hover:bg-sky-500 hover:text-gray-50 focus:ring-2 focus:ring-gray-50">
                <svg class="w-7 h-7" fill="none" stroke="currentColor" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"></path>
                </svg>
            </button>
            <div class="-mr-4 flex items-center"> 
                <a href="#" class="text-4xl font-bold text-gray-950">SiaC</a>
                <div class="ml-8 space-x-9">
                    <a class="px-3 py-2 text-blue-600" href="#" class="py-2">Administradores</a>
                    <a class="px-3 py-2 text-slate-600 transition-colors hover:text-blue-600" href="#">Pendientes</a>
                    <a class="px-3 py-2 text-slate-600 transition-colors hover:text-blue-600" href="#">Usuarios</a>
                    <a class="px-3 py-2 text-slate-600 transition-colors hover:text-blue-600" href="#">Ayuda</a>            
                </div>
            </div>
            <div class="flex">
                <button>Ian@gmail.com</button>
                <button class="p-2 justify-between"> 
                    <img class="h-9 w-9 rounded-full" src="Perfil_sin_foto.png" alt="perfil"/>
                </button>
            </div>
        </div>
    </header>
    <main class="p-5 m-5">
        <article class="rounded bg-white shadow">
            <div class="space-y-8 p-8">
                <div class="flex flex-col items-center space-y-5">
                    <div class="flex flex-col space-y-10">
                        {{#each usuarios}}
                            <div class="flex items-center space-x-10">
                                <p>{{this.correo_electronico}}</p>
                                <button class="ml-1 px-1 py-1 text-sky-900 bg-blue-100 transition-colors hover:bg-blue-300 focus:ring-2 focus:ring-gray-100 shadow-md">Editar</button>
                                <button class="ml-1 px-1 py-1 text-sky-900 bg-indigo-200 hover:bg-indigo-300 focus:ring-2 focus:ring-gray-100 shadow-md">Eliminar</button>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
            <form action="/altaAdm">
            <button type="submit">Alta administrador</button>
            </form>
        </article>
    </main>
</body>